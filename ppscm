pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World have a faith in you'
            }
        }
        stage('Build') {
            steps {
                echo 'building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deploying'
            }
        }
         stage('Test') {
            steps {
                echo 'testing'
            }
        }
        stage('Release') {
            steps {
                echo 'releasing'
            }
        }
    }
}
